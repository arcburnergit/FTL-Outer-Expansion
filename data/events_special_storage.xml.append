    <event load="STORAGE_CHECK_AEA_O2SYS" />
  </mod-append:choice>
  <mod-append:choice req="AEA_ACID_O2SYS_OFF" blue="true" hidden="true">
    <text>Acidic Filtering System.</text>
    <event load="STORAGE_CHECK_AEA_O2SYS_OFF" />
  </mod-append:choice>
  <mod-append:choice req="AEA_NECRO_CONVERT" blue="true" hidden="true">
    <text>Heretic Converter.</text>
    <event load="STORAGE_CHECK_AEA_NECRO_CONVERT" />
  </mod-append:choice>
  <mod-append:choice req="AEA_CULTISTS" blue="true" hidden="true">
    <text>Cultists.</text>
    <event load="STORAGE_CHECK_AEA_CULTIST_WIZARD" />
  </mod-append:choice>
  <mod-append:choice req="AEA_TINY" blue="true" hidden="true">
    <text>Tiny Cultists.</text>
    <event load="STORAGE_CHECK_AEA_CULTIST_TINY" />
  </mod-append:choice>
  <mod-append:choice req="LIST_WEAPON_CULT" blue="true" hidden="true">
    <text>Cult Weapons.</text>
    <event load="STORAGE_CHECK_AEA_CULTIST_LASER" />
  </mod-append:choice>
  <mod-append:choice req="SHIP_AEA_BROADSIDE3" blue="true" hidden="true">
    <text>Broadside Artillery.</text>
    <event load="AEA_BROADSIDE3_MAIN" />
  </mod-append:choice>
</mod:findName>

<mod:findName type="event" name="STORAGE_CHECK_SYSTEM">
	<mod-append:choice req="aea_super_shields" blue="true" hidden="true">
	  <text>Auxiliary Shields.</text>
	  <event load="STORAGE_CHECK_SYSTEM_AEA_SUPER_SHIELDS" />
	</mod-append:choice>
</mod:findName>

<event name="STORAGE_CHECK_SYSTEM_AEA_SUPER_SHIELDS">
    <text>What upgrade do you want to install?</text>
    <choice req="UPG_AEA_SUPER_SHIELD_OVERCHARGER" lvl="1" max_group="0" blue="false" hidden="true">
        <text>Shield Overcharger. [ALREADY INSTALLED]</text>
        <event load="OPTION_INVALID" />
    </choice>
    <choice req="UPG_AEA_SUPER_SHIELD_OVERCHARGER" lvl="0" max_group="0" blue="false" hidden="true">
        <text>Shield Overcharger. [Cost: 50~, 1{]</text>
        <event load="STORAGE_CHECK_SYSTEM_AEA_SUPER_SHIELD_OVERCHARGER" />
    </choice>
    <choice>
        <text>Nevermind.</text>
        <event load="STORAGE_CHECK_SYSTEM_LOAD" />
    </choice>
</event>
<event name="STORAGE_CHECK_SYSTEM_AEA_SUPER_SHIELD_OVERCHARGER">
	<text>You are about to install the Shield Overcharger upgrade.
	[Effects: Grants all super shield layers that the system can generate on jump]</text>
	<choice max_group="0" blue="false" hidden="true">
		<text>Perform the upgrade. [Cost: 50~, 1{]</text>
		<event>
			<text>You install the upgrade.</text>
			<item_modify>
				<item type="fuel" min="-1" max="-1" />
				<item type="scrap" min="-50" max="-50" />
			</item_modify>
			<choice hidden="true">
				<text>Continue...</text>
				<event load="INSTALL_AEA_SUPER_SHIELD_OVERCHARGER" />
			</choice>
		</event>
	</choice> 
	<choice req="UPG_AEA_SUPER_SHIELD_OVERCHARGER_EX" max_group="1" blue="false" hidden="true">
		<text>Install the External Augment internally.</text>
		<event>
			<text>You install the upgrade.</text>
			<remove name="UPG_AEA_SUPER_SHIELD_OVERCHARGER_EX" />
			<choice hidden="true">
				<text>Continue...</text>
				<event load="INSTALL_AEA_SUPER_SHIELD_OVERCHARGER" />
			</choice>
		</event>
	</choice>
	<choice req="pilot" lvl="1" max_group="2" blue="false" hidden="true">
		<text>Nevermind.</text>
		<event load="STORAGE_CHECK_SYSTEM_LOAD" />
	</choice>
</event>
<event name="INSTALL_AEA_SUPER_SHIELD_OVERCHARGER">
	<variable name="loc_internal_upgrades" op="add" val="1" />
	<hiddenAug>UPG_AEA_SUPER_SHIELD_OVERCHARGER</hiddenAug>
	<loadEvent>STORAGE_CHECK_SYSTEM</loadEvent>
</event>

<mod:findName type="event" name="STORAGE_CHECK_LAB_ORCHID">
	<mod:findWithChildLike type="choice" child-type="text">
      <mod:selector>Go back.</mod:selector>
      <mod:setAttributes max_group="997"/>
    </mod:findWithChildLike>
	<mod-append:choice req="LAB_ORCHID_AEA_COMMUNION" lvl="1" max_group="7" blue="false" hidden="true">
		<text>Sec [Cultist]: Ancient Communion. [An upgrade is already installed]
		[You can purchase spells at empty beacons].</text>
		<event load="OPTION_INVALID" />
	</mod-append:choice>
	<mod-append:choice req="LAB_ORCHID_AEA_COMMUNION" lvl="0" max_group="7" blue="false" hidden="true">
		<text>Sec [Cultist]: Ancient Communion. [Cost: 35~, 4{].</text>
		<event load="STORAGE_CHECK_LAB_AEA_COMMUNION" />
	</mod-append:choice>
</mod:findName>

<mod:findName type="event" name="STORAGE_CHECK_AUG">
	<mod-append:choice req="LAB_ORCHID_AEA_COMMUNION">
        <text>(Ancient Communion) Commune.</text>
        <event>
			<store>STORE_AEA_COMMUNION</store>
        </event>
    </mod-append:choice>
	<mod-append:choice req="EX_LAB_ORCHID_AEA_COMMUNION">
        <text>(Ancient Communion) Commune.</text>
        <event>
			<store>STORE_AEA_COMMUNION</store>
        </event>
    </mod-append:choice>
</mod:findName>


<event name="STORAGE_CHECK_LAB_AEA_COMMUNION">
	<text>You are about to install the Ancient Communion modification.
	[Effects: You can purchase spells at empty beacons]
	
	[Warning: This will affect Cultist Crew only. You will be able to install other secondary mods and primary mods afterwards.].</text>
	<choice hidden="true" req="LAB_UPGRADE" lvl="1" max_group="0" blue="false">
		<text>Install the modification. [Cost: 35~, 4{].</text>
		<event>
			<text>You install the modification.</text>
			<item_modify>
				<item type="scrap" min="-35" max="-25" />
				<item type="fuel" min="-4" max="-4" />
			</item_modify>
			<choice hidden="true">
				<text>Continue...</text>
				<event load="INSTALL_LAB_AEA_COMMUNION" />
			</choice>
		</event>
	</choice>
	<choice req="LAB_UPGRADE" lvl="0" max_lvl="2" max_group="0" blue="false" hidden="true">
		<text>You must have the lab upgrade for this modification.</text>
		<event load="OPTION_INVALID" />
	</choice>
	<choice req="pilot" lvl="1" max_group="1" blue="false" hidden="true">
		<text>Nevermind.</text>
		<event load="STORAGE_CHECK_LAB_LOAD" />
	</choice>
</event>
<event name="INSTALL_LAB_AEA_COMMUNION">
	<hiddenAug>LAB_ORCHID_AEA_COMMUNION</hiddenAug>
	<loadEvent>STORAGE_CHECK_LAB_ORCHID</loadEvent>
</event>

